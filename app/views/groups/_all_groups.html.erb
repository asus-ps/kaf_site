    <table class="TableSimple">

      <% @specialities.each do |s| %>
        <% (1..(s.count_years)).each do |i|  %>
          <tr>
            <td><%= link_to("#{s.short_name}-#{i}", students_from_group_path(s,(Time.now.year - i))) %></td>
          </tr>
        <% end %>
      <% end %>
    </table>
